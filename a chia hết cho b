<script type="text/javascript">
	var x = parseInt(prompt('Nhập vào số a:'));
	var y = parseInt(prompt('Nhập vào số b:'));
	if (x % y ==0){
		alert('a chia hết cho b');
	}else {
		alert('a không chia hết cho b');
	}
</script>	
