<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
<script type="text/javascript">
	var x = parseInt(prompt('Nhập vào số bất kỳ:'));
	var y = parseInt(prompt('Nhập vào số bất kỳ:'));
	var z = parseInt(prompt('Nhập vào số bất kỳ:'));
	if (x >y){
		if (x>z){
			alert(x + 'là số lớn nhất');
		}else {
			alert(z + 'là số lớn nhất');
		}
	}else {
		if (x<y){
			if (y>z){
				alert(y + 'là số lớn nhất');
			}else{
				alert(z + 'là số lớn nhất');
			}
		}
	}
</script>	
</body>
</html>
